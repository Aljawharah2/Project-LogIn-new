<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profileActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="1301dp"
        android:layout_height="965dp"
        android:layout_marginStart="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.23"
        app:srcCompat="@drawable/whatsapp_image_2023_05_10_at_11_56_39_am" />

    <RelativeLayout
        android:layout_width="152dp"
        android:layout_height="246dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="170dp"
        android:layout_marginEnd="25dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toStartOf="@+id/relativeLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="120dp"
            android:layout_height="59dp"
            android:layout_alignParentTop="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="157dp"
            android:layout_marginRight="9dp"
            android:layout_marginBottom="50dp"
            android:fontFamily="serif"
            android:outlineSpotShadowColor="@color/black"
            android:shadowColor="@color/black"
            android:text="Blue Diamond Ring \n 10000SR"
            android:textColor="#414140"
            android:textColorHighlight="#B700BCD4"
            android:textSize="13sp" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="38dp"
            android:layout_height="35dp"
            android:layout_marginStart="90dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="190dp"
            android:layout_marginRight="9dp"
            android:layout_marginBottom="100dp"
            android:visibility="visible"
            app:srcCompat="@drawable/baseline_delete_24" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="86dp"
            android:layout_height="190dp"
            android:layout_marginStart="20dp"
            app:srcCompat="@drawable/my_project__1_" />

    </RelativeLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#1C201F"
        app:itemIconTint="@color/white"
        app:itemTextColor="#FEFEFE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_menu">

    </com.google.android.material.bottomnavigation.BottomNavigationView>

    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

    </FrameLayout>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="165dp"
        android:layout_height="42dp"
        android:layout_marginStart="480dp"
        android:layout_marginTop="88dp"
        android:text="Welcome!"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_wrapBehaviorInParent="horizontal_only" />

    <Button
        android:id="@+id/logout"
        android:layout_width="92dp"
        android:layout_height="35dp"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="540dp"
        android:background="#ffffff"
        android:text="Log out"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="152dp"
        android:layout_height="246dp"
        android:layout_marginTop="170dp"
        android:layout_marginEnd="35dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="110dp"
            android:layout_height="186dp"
            android:layout_marginStart="20dp"
            app:srcCompat="@drawable/my_project__2_" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="38dp"
            android:layout_height="35dp"
            android:layout_marginStart="90dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="190dp"
            android:layout_marginRight="9dp"
            android:layout_marginBottom="100dp"
            android:visibility="visible"
            app:srcCompat="@drawable/baseline_delete_24" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="120dp"
            android:layout_height="49dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="157dp"
            android:layout_marginRight="9dp"
            android:layout_marginBottom="50dp"
            android:fontFamily="serif"
            android:outlineSpotShadowColor="@color/black"
            android:shadowColor="@color/black"
            android:text="Emerald Green Earrings \n 12000"
            android:textColor="#414140"
            android:textColorHighlight="#B700BCD4"
            android:textSize="11sp" />

    </RelativeLayout>


    <Button
        android:id="@+id/Addme"
        android:layout_width="97dp"
        android:layout_height="93dp"
        android:layout_marginStart="300dp"
        android:layout_marginTop="572dp"
        android:background="#00000000"
        android:drawableStart="@drawable/addm"
        android:text="."
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>